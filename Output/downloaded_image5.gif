<script>
    $(document).ready(function() {

        var pages = [
            'http://www.nhs.uk/Conditions/Stroke/Pages/Introduction.aspx'
            , 'http://www.nhs.uk/conditions/Stroke/Pages/treatment.aspx'
            , 'http://www.nhs.uk/Conditions/Stroke/Pages/recovery.aspx'
            , 'http://www.nhs.uk/Conditions/Parkinsons-disease/Pages/Introduction.aspx'
            , 'http://www.nhs.uk/Conditions/Parkinsons-disease/Pages/Treatment.aspx'
            , 'http://www.nhs.uk/Conditions/Parkinsons-disease/Pages/Livingwith.aspx'
            , 'http://www.nhs.uk/Conditions/hip-fracture/Pages/introduction.aspx'
            , 'http://www.nhs.uk/Conditions/hip-fracture/Pages/treatment.aspx'
            , 'http://www.nhs.uk/Conditions/hip-fracture/Pages/recovery.aspx'
            , 'http://www.nhs.uk/Conditions/multiple-sclerosis/Pages/Introduction.aspx'
            , 'http://www.nhs.uk/Conditions/Multiple-sclerosis/Pages/Treatment.aspx'
            , 'http://www.nhs.uk/Conditions/Multiple-sclerosis/Pages/Living-with.aspx'
            , 'http://www.nhs.uk/Conditions/downs-syndrome/Pages/Introduction.aspx'
            , 'http://www.nhs.uk/Conditions/Downs-syndrome/Pages/Treatment.aspx'
            , 'http://www.nhs.uk/Conditions/Downs-syndrome/Pages/Symptoms.aspx'
            , 'http://www.nhs.uk/conditions/autistic-spectrum-disorder/pages/introduction.aspx'
            , 'http://www.nhs.uk/Conditions/Autistic-spectrum-disorder/Pages/Treatment.aspx'
            , 'http://www.nhs.uk/Conditions/Autistic-spectrum-disorder/Pages/Adultswithautism1.aspx'
        ];

        $.each(pages, function(index, element) {
            var path = window.location.href;
            if (path.toLowerCase() == element.toLowerCase()) {
                insertSharingTags();
                insertInlineLinksTags();
                insertUsefulLinksTags();
                insertExternalLinksTags();
                insertSubHeadingTags();
                insertHeadingTags();
                insertHeadingTags();
                insertImageLinksTags();
                insertFooterTags();
                insertForumLinkTags();

            }
        });


    });

    var dcsText;
    var count;




    function insertInlineLinksTags() {
        var pageBody = $('.main-content');
        var links = $(pageBody).children('p,ul,div').find('a');
        dcsText = 'DCSext.CareInLine';
        count = 1;
        links.map(function (index, element) {
            var tagString = buildWebTrendLink(dcsText, element);
            count += 1;
            $(element).attr('onclick', tagString.toString());
            $(element).bind('click', tagString.toString());

            // $(element).attr('onclick',tagString);	WORKS together
            // $(element).bind('click',tagString.toString());  WORKS together

        });
    }

    function insertUsefulLinksTags() {
        var usefulLinksSection = $('.panel-top').find('ul').get(0);
        var usefulLinks = $(usefulLinksSection).children().find('a');
        dcsText = 'DCSext.CareUseful';
        count = 1;
        usefulLinks.each(function (index, element) {
            var tagString = buildWebTrendLink(dcsText, element);
            count += 1;
            $(element).attr('onclick', tagString);
            $(element).bind('click', tagString.toString());
        });
    }

    function insertExternalLinksTags() {
        var externalLinksSection = $('.panel-top').find('ul').get(1);
        var externalLinks = $(externalLinksSection).children().find('a');
        dcsText = 'DCSext.CareExternal';
        count = 1;
        externalLinks.each(function (index, element) {
            var tagString = buildWebTrendLink(dcsText, element);
            count += 1;
            $(element).attr('onclick', tagString);
            $(element).bind('click', tagString.toString());
        });
    }

    function insertSubHeadingTags() {
        var subHeadingSection = $('#ctl00_PlaceHolderMain_articles').find('ul');
        var subHeadingLinks = $(subHeadingSection).children().find('a');
        dcsText = 'DCSext.CareSubHeader';
        count = 1;
        subHeadingLinks.each(function (index, element) {
            var tagString = buildWebTrendLink(dcsText, element);
            count += 1;
            $(element).attr('onclick', tagString);
            $(element).bind('click', tagString.toString());
        });
    }

    function insertHeadingTags() {
        var headingSection = $('#ctl00_PlaceHolderMain_topicHeadings').find('ul');
        var headingLinks = $(headingSection).children('li').find('a');
        dcsText = 'DCSext.CareHeader';
        count = 1;
        headingLinks.each(function (index, element) {
            var tagString = buildWebTrendLink(dcsText, element);
            count += 1;
            $(element).attr('onclick', tagString);
            $(element).bind('click', tagString.toString());
        });
    }

    function insertImageLinksTags() {
        var imageLinksSection = $('#webZoneRight').find('.image');
        var imageLinks = $(imageLinksSection).find('a');
        var imageTextLinksSection = $('#webZoneRight').find('.panel-text');
        var imageTextLinks = $(imageTextLinksSection).find('h2').find('a');
        dcsText = 'DCSext.CareSideImage';
        count = 1;
        imageLinks.each(function (index, element) {
            if ($(imageTextLinks[index]).attr('href') == $(element).attr('href')) {
                var tagString = buildWebTrendLink(dcsText, imageTextLinks[index]);
                $(element).attr('onclick', tagString);
                count += 1;
                $(imageTextLinks[index]).attr('onclick', tagString);
                $(imageTextLinks[index]).bind('click', tagString.toString());
            }
        });
    }

    function insertFooterTags() {
        var footerImages = $('.footer-logos').find('ul');
        var footerImagesLinks = $(footerImages).children().find('a');
        dcsText = 'DCSext.CareFootImage';
        count = 1;
        footerImagesLinks.each(function (index, element) {
            var tagString = buildWebTrendLink(dcsText, element);
            count += 1;
            $(element).attr('onclick', tagString);
            $(element).bind('click', tagString.toString());
        });
    }

    function insertSharingTags() {
        var sharingLinkDivs = $('.social-sharing');
        var sharingHeaderLinks = $(sharingLinkDivs[0]).find('ul').children();
        var sharingFooterLinks = $(sharingLinkDivs[1]).find('ul').children();
        dcsText = 'DSCtext.CareShare';
        count = 1;
        sharingHeaderLinks.find('a').each(function (index, element) {
            var tagString = buildWebTrendLinkForSharingLinks(dcsText, element, 'Header');
            count += 1;
            $(element).attr('onclick', tagString);
            $(element).bind('click', tagString);
        });
        sharingFooterLinks.find('a').each(function (index, element) {
            var tagString = buildWebTrendLinkForSharingLinks(dcsText, element, 'Footer');
            count += 1;
            $(element).attr('onclick', tagString);
            $(element).bind('click', tagString);
        });
    }

    function insertForumLinkTags() {
        $('#webZoneLeft').ready(function () {
            setTimeout(function() {
                var forumLinks = $('#webZoneLeft .healthunlocked_wrapper a');
                dcsText = 'DCSext.CareForum';
                count = 1;
                forumLinks.each(function(index, element) {
                    var tagString = buildWebTrendLink(dcsText, element);
                    count += 1;
                    $(element).attr('onclick', tagString);
                    $(element).bind('click', tagString.toString());
                });
            }, 1000);
        });
    }

    function buildWebTrendLinkForSharingLinks(dcsText, element, location) {
        var className = $(element).parent().attr('class');
        var dynamicText;
        if (location == 'Header') {
            dynamicText = 'Header_';
        } else {
            dynamicText = 'Footer_';
        }
        switch (className) {
            case 'email':
                dynamicText += 'Email';
                break;
            case 'twitter':
                dynamicText += 'Twitter';
                break;
            case 'facebook':
                dynamicText += 'FB';
                break;
            case 'google':
                dynamicText += 'Google Bookmark';
                break;
            case 'nhsc-save':
                dynamicText += 'Choices Bookmark';
                break;
            case 'print-ip':
                dynamicText += 'Print';
                break;
        }
        return "dcsMultiTrack" + linkBuilder(dcsText, dynamicText);

    }

    function buildWebTrendLink(dcsText, element) {
        var dynamicText = $(element).text();
        if (dcsText == 'DCSext.CareUseful' ||
            dcsText == 'DCSext.CareExternal' ||
            dcsText == 'DCSext.CareSideImage' ||
            dcsText == 'DCSext.CareFootImage') {
            dynamicText = 'Link ' + count + '-' + $(element).text();

        } else if (dcsText == 'DCSext.CareHeader' ||
            dcsText == 'DCSext.CareSubHeader' ||
            dcsText == 'DCSext.CareSubHeader' ||
            dcsText == 'DCSext.CareInLine') {
            dynamicText = $(element).text();
        } else if (dcsText == 'DCSext.CareForum') {
            if ($(element).text() == 'More from the community') {
                dynamicText = 'More from the community';
            } else {
                dynamicText = 'Link ' + count;
            }

        }
        return "dcsMultiTrack" + linkBuilder(dcsText, dynamicText);
    }

    function linkBuilder(dcsText, dynamicText) {
        return "\(" + "\'" + dcsText + "\'" + "," + "\'" + dynamicText +
            "\'" + "," + "\'WT.dl\'" + "," + "\'121\'" + "\)";


    }

<script type="text/javascript">

var funnelStep = function () {
	var thisFunnelStep = "";
	if(jQuery("#stage1").is(":visible")) {
		thisFunnelStep = "Viewed Funnel Step One";
	} else if (jQuery("#stage2").is(":visible")) {
		thisFunnelStep = "Viewed Funnel Step Two";
	} else if (jQuery("#stage3").is(":visible")) {
		thisFunnelStep = "Viewed Funnel Step Three";
	} else if (jQuery("#stage4").is(":visible")) {
		thisFunnelStep = "Viewed Funnel Step Four";
	} else if (jQuery("#stage5").is(":visible")) {
		thisFunnelStep = "Viewed Funnel Step Five";
	} else {
		// Lets keep looking... current stepText will be of the last step
		try {
			var thisStepText = jQuery("#stepText").text();
			if(thisStepText.indexOf("Step 1:") != -1){
				thisFunnelStep = "Viewed Funnel Step Two";
			} else if (thisStepText.indexOf("Step 2:") != -1) {
				thisFunnelStep = "Viewed Funnel Step Three";
			} else if (thisStepText.indexOf("Step 3:") != -1) {
				thisFunnelStep = "Viewed Funnel Step Four";
			} else if (thisStepText.indexOf("Step 4:") != -1) {
				thisFunnelStep = "Viewed Funnel Step Five";
			};
		} catch (err){
			if(console){ 
				console.log("ISSUE WITH KM TRACKING",err );
			};
		};
 	};

 	
		
		
		

	if(console){ console.log("STEP::",thisFunnelStep );};
	if(thisFunnelStep != ""){
		var reportAddToFavorites = newKissSl(kisssl);
		reportAddToFavorites.init({
		    "SiteCatalyst": (typeof s !== 'undefined')?s:{},
		    callback: function(){
		        var globalVars = this.userDataObj;
		        if(console){
		            console.log("globalVars: ", globalVars);
		        };
		        _kmq.push(['record', thisFunnelStep, globalVars]);
		    }
		});
	}; 
};

jQuery(function(){
	/* This is to track the first funnel step viewed */
	funnelStep();
});



function destroyExtraDivs(divID) {

divCount = 0;
theCounter = $('div[id=' + divID + ']').size();
$('div[id=' + divID + ']').each(

	function() {
	
		divCount++;
		
		if(divCount < theCounter) {
		$(this).remove();
		}
	
	}

);

}


function validateCVVCode() {
	var cvvCode = document.getElementById('cvv').value;
	cvvCode = stripAlphaChars(cvvCode);
	document.getElementById('cvv').value = cvvCode;

	var cardNum = document.getElementById('cc').value;
	
	var lengthTest = 3;

	if (cardNum.length > 0) {
		if(cardNum.substring(0,1)=="3") {
		lengthTest = 4;
		}
	}
	
	if(cvvCode.length < lengthTest) {
	return false;
	}
	else {
	return true;
	}

}


function stripAlphaChars(pstrSource)
{
var m_strOut = new String(pstrSource);
m_strOut = m_strOut.replace(/[^0-9]/g, '');
return m_strOut;
}


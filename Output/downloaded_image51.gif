 var countdownSecs = 0;
 
 function processCountDown() {
 
 if(processCompleted == false) {
 
	 if(countdownSecs <= 120) {
	 $('#subFormElements').html('<div style="padding-top:100px;padding-left:40px;"><b>Processing your transaction</b> <img id="loader" src="/scripts/ajax-loader.gif" width="18px" height="18px"><br /><br />Please allow at least 120 seconds for this transaction to complete.<br /><br /><br /><b>Elapsed Processing Time:</b> ' + countdownSecs + ' seconds.</div>');
	
	setTimeout("processCountDown()",1000);
	}
	
	else {
	$('#subFormElements').html('<div style="padding-top:100px;padding-left:40px;"><b>Processing your transaction</b> <img id="loader" src="/scripts/ajax-loader.gif" width="18px" height="18px"><br /><br />Please allow at least 120 seconds for this transaction to complete.<br /><br /><br /><b>Elapsed Processing Time:</b> Timeout reached...</div>');
	}

}

countdownSecs = countdownSecs + 1;

 }



